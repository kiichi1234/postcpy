<h1>相談内容投稿ページ</h1>
<%= form_with model: @post do |f| %>
    <%= f.label :カテゴリー %>
    <%= f.select :category, [["身体的", "身体的"], ["精神的", "精神的"], ["生活面", "生活面"], ["気付き", "気付き"], ["その他", "その他"]], include_blank: "選択して下さい" %>
    <%= f.label :本文　%>
    <%= f.text_area :body %>
    <%= f.submit '投稿する' %>
<% end %>
<%= link_to "ログアウト", logout_path, data: {turbo_method: :delete} %>
<%= link_to "あなたの投稿", user_path(current_user) %>